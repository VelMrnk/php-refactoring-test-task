{
    "name": "valentinemurnik/commission-rate",
    "description": "Refactoring the service to calculate transaction fees.",
    "type": "project",
    "config": {
        "platform": {
            "php": "7.4"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "7.4",
        "ext-json": "*",
        "symfony/filesystem": "^5.1",
        "symfony/http-client": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/var-dumper": "^5.1",
        "mikey179/vfsstream": "^1.6"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
